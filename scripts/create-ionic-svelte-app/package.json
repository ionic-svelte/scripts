{
  "name": "@ionic-svelte/create-app",
  "version": "0.0.38",
  "description": "Use this CLI app to setup a new Ionic application in a new SvelteKit project.",
  "keywords": [
    "ionic",
    "svelte",
    "sveltekit",
    "svelte-kit"
  ],
  "homepage": "https://ionic-svelte.firebaseapp.com",
  "repository": {
    "type": "git",
    "url": "https://github.com/ionic-svelte/create-app.git"
  },
  "license": "MIT",
  "author": "Tommertom",
  "type": "module",
  "main": "./src/index.js",
  "bin": {
    "create-ionic-svelte-app": "./src/index.js"
  },
  "files": [
    "src/**"
  ],
  "engines": {
    "node": ">=14.16"
  },
  "scripts": {
    "dev": "node src/index.js",
    "long": "node src/index.js --types=typescript --prettier --eslint --playwright=false -f svelte-kit --ionicons=false",
    "pub:beta": "npm publish --tag beta",
    "pub:next": "npm publish --tag next",
    "pub:release": "npm publish",
    "release": "npm publish",
    "start": "node dist/index.js"
  },
  "publishConfig": {
    "access": "public"
  },
  "dependencies": {
    "eslint": "^9.23.0",
    "fs-extra": "^10.1.0",
    "ip": "^1.1.8",
    "kleur": "^4.1.5",
    "mri": "^1.2.0",
    "prompts": "^2.4.2",
    "sv": "^0.7.1"
  },
  "devDependencies": {
    "bumpp": "^8.2.1",
    "standard-version": "^9.5.0",
    "svelte": "^4.2.7"
  }
}
